df['TotalAmount'] = df['MntWines'] + df['MntFruits'] + df['MntMeatProducts'] + df['MntFishProducts'] + df['MntSweetProducts'] + df['MntGoldProds']
df['Promotion'] = df['Response'].map({1: 'Promo', 0: 'Non Promo'})

